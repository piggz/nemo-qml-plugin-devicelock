<node name="/lockcode">
 <interface name="org.nemomobile.devicelock.LockCodeSettings">
  <property name="LockCodeSet" type="b"/>
  <method name="Change">
   <arg name="old_code" type="s" direction="in"/>
   <arg name="new_code" type="s" direction="in"/>
  </method>
  <method name="Clear">
   <arg name="code" type="s" direction="in"/>
  </method>
 </interface>
</node>
